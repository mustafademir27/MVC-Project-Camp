@*/Content/GetAllContent/*@

@using EntityLayer.Concrete
@model List<Content>
@{
    ViewBag.Title = "GetAllContent";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@using (Html.BeginForm("GetAllContent", "Content", FormMethod.Post))
            {
                <p>
                    Aranacak Kelimeyi Girin : @Html.TextBox("p") <!--Controller tarafında p parametresi verdiğim için burada da p veriyorum-->
                    <input type="submit" value="Ara"/>
                </p>
            }



@foreach(var item in Model)
            {
                <p>@item.ContentValue</p>
            }

